    <script>
        $(document).ready(function () {
            $('#slick-slide').slick({
                slidesToShow: 3, // 4개의 카드를 가로로 나열하도록 설정 (원하는 개수로 변경 가능)
                nextArrow: $('#custom-next'),
                prevArrow: $('#custom-prev'),
            });
        });
        const slickSlide = jQuery('#slcik-slide')


        if (slickSlide) {
            slickSlide.slick({
                dots: true,
                arrows: false,
                slidesToShow: 3,
                slideToScroll: 1,
                autoPlay: true,
                autoplaySpeed: 3000,
                responsive: [
                    {
                        breakpoint: 768,
                        settings: {
                            slidesToShow: 2
                        }
                    },
                    {
                        breakpoint: 576,
                        settings: {
                            slidesToShow: 1
                        }
                    }
                ]
            })

        }
